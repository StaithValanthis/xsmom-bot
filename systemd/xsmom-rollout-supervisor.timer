[Unit]
Description=Run xsmom-bot rollout supervisor every 6 hours
Requires=xsmom-rollout-supervisor.service

[Timer]
OnCalendar=*-*-* 00:00:00
OnCalendar=*-*-* 06:00:00
OnCalendar=*-*-* 12:00:00
OnCalendar=*-*-* 18:00:00
AccuracySec=5m
Persistent=true
RandomizedDelaySec=600

[Install]
WantedBy=timers.target

